<footer id="page-footer" class="p-y-1 bg-inverse">

  <div class="container">
    <div id="course-footer">{{{ output.course_footer }}}</div>


    <div class="footerUM row">
      <div class="col-lg-4 hidden-md-down logoUmPrn">
        <a href="http://www.univ-lemans.fr/">
            {{# pix }} logoUM, theme, Le Mans Université{{/ pix }}
          </a>
        <a href="http://prn.univ-lemans.fr/">
            {{# pix }} logoPRN, theme, Pôle Ressources Numériques Le Mans Université{{/ pix }}
          </a>

      </div>


      <div class="col-lg-4 footerdivEAD">

        {{# pix }} logo80px, theme, EADUM{{/ pix }}
        <ul>
          <li>

            {{# str }} sitetitle, theme_eadumboostvaleo {{/ str }}
          </li>

        </ul>

      </div>

      <div class="col-lg-4 textfooter ">

        {{# pix }} valeo, theme, Valéo{{/ pix }}


        {{# pix }} lm_acoustique, theme, Le Mans Acoustique{{/ pix }}

      </div>
    </div>


    <div class="col-md-12 hidden-lg-up logos-mobile">
      <br />
      <a href="http://webmail.univ-lemans.fr/">
        {{# str }} mail, theme_eadumboostvaleo {{/ str }}
      </a>

    </div>



    {{# output.page_doc_link }}
      <p class="helplink">{{{ output.page_doc_link }}}</p>
    {{/ output.page_doc_link }}


    <nav class="nav navbar-nav hidden-lg-up">

      {{# output.custom_menu_flat }}
        <ul class="list-unstyled p-t-1">
          {{> theme_boost/custom_menu_footer }}
        </ul>
      {{/ output.custom_menu_flat }}
    </nav>
    {{{ output.standard_footer_html }}} {{{ output.standard_end_of_body_html }}}

    <div class="col-md-12 hidden-lg-up logos-mobile">
      <a href="http://www.univ-lemans.fr/">
        {{# pix }} logoUM, theme, Le Mans Université{{/ pix }}
      </a>
      <a href="http://prn.univ-lemans.fr/">
        {{# pix }} logoPRN, theme, Pôle Ressources Numériques Le Mans Université{{/ pix }}
      </a>

    </div>


  </div>
</footer>

{{#js}}
  /* add target blank @ "mail" menu */
  require(['jquery', 'jqueryui'], function($, jqui) {
    $("header.navbar a[title='Boîte mail UM']").attr('target' ,'_blank');
    $("header.navbar a[title='Mailbox']").attr('target' ,'_blank');
  });
{{/js}}
